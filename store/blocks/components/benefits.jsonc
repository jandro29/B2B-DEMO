{
  "flex-layout.row#benefits": {
    "props": {
      "blockClass": "benefits"
    },
    "children": ["flex-layout.col#benefits"]
  },
  "flex-layout.col#benefits": {
    "props": {},
    "children": [ "slider-layout#benefits"] //"rich-text#whyShopWithVTEX",
  },

  /*"rich-text#whyShopWithVTEX": {
    "props": {
      "text": "### Why shop with VTEX",
      "blockClass": "sectionTitle"
    }
  },*/

  "slider-layout#benefits": {
    "props": {
      "itemsPerPage": {
        "desktop": 4,
        "tablet": 2,
        "phone": 2
      },
      "infinite": false,
      "showNavigationArrows": "never",
      "blockClass": "benefitsSlider"
    },
    "children": [
      "flex-layout.row#benefitRow1",
      "flex-layout.row#benefitRow2",
      "flex-layout.row#benefitRow3",
      "flex-layout.row#benefitRow4"
    ]
  },
  "flex-layout.row#benefitRow1": {
    "children": ["image#benefit1", "rich-text#benefits1"],
    "props": {
      "blockClass": "flex-column",
      "preventHorizontalStretch": true
    }
  },
  "flex-layout.row#benefitRow2": {
    "children": ["image#benefit2", "rich-text#benefits2"],
    "props": {
      "blockClass": "flex-column",
      "preventHorizontalStretch": true
    }
  },
  "flex-layout.row#benefitRow3": {
    "children": ["image#benefit3", "rich-text#benefits3"],
    "props": {
      "blockClass": "flex-column",
      "preventHorizontalStretch": true
    }
  },
  "flex-layout.row#benefitRow4": {
    "children": ["image#benefit4", "rich-text#benefits4"],
    "props": {
      "blockClass": "flex-column",
      "preventHorizontalStretch": true
    }
  },
  "image#benefit1": {
    "props": {
      "src": "assets/placeholders/servicio-cliente.svg",
      "blockClass": "benefitImage"
    }
  },
  "image#benefit2": {
    "props": {
      "src": "assets/placeholders/nuestras-tiendas.svg",
      "blockClass": "benefitImage"
    }
  },
  "image#benefit3": {
    "props": {
      "src": "assets/placeholders/envio-domicilio.svg",
      "blockClass": "benefitImage"
    }
  },
  "image#benefit4": {
    "props": {
      "src": "assets/placeholders/retiro-tienda.svg",
      "blockClass": "benefitImage"
    }
  },
  "rich-text#benefits1": {
    "props": {
      "text": "**Retiro en tienda** Recoge tu pedido en nuestro almacén sin costo adicional.",
      "blockClass": "benefitsItem"
    }
  },
  "rich-text#benefits2": {
    "props": {
      "text": "**Envió a domicilio** Recibe tus productos en obra, oficina o almacén.",
      "blockClass": "benefitsItem"
    }
  },
  "rich-text#benefits3": {
    "props": {
      "text": "**Nuestras tiendas** Asesoría técnica y compras presenciales.",
      "blockClass": "benefitsItem"
    }
  },
  "rich-text#benefits4": {
    "props": {
      "text": "**Servicio al cliente** Soporte técnico y postventa garantizada.",
      "blockClass": "benefitsItem"
    }
  }
}
