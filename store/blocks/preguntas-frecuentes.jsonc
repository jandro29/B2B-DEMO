{
  "store.custom#preguntas-frecuentes": {
    "blocks": ["responsive-layout.desktop#preguntas-frecuentes", "responsive-layout.mobile#preguntas-frecuentes"]
  },

  "responsive-layout.desktop#preguntas-frecuentes": {
    "children": [
        "flex-layout.row#titulo-preguntas-frecuentes",
        "disclosure-layout-group#group"
    ]
  },



  "responsive-layout.mobile#preguntas-frecuentes": {
    "children": [
        "flex-layout.row#titulo-preguntas-frecuentes",
        "disclosure-layout-group#group"
    ]
  },

  "flex-layout.row#titulo-preguntas-frecuentes":{
    "children":["rich-text#titulo-preguntas-frecuentes"]
  },

  "rich-text#titulo-preguntas-frecuentes":{
    "props":{
      "text":"Preguntas Frecuentes",
      "textAlignment":"CENTER",
      "textPosition":"CENTER",
      "font":"t-heading-1"
    }
  },



  "disclosure-layout-group#group": {
    "children": ["disclosure-layout#first", "disclosure-layout#second","disclosure-layout#third"]
    
  },

  "disclosure-layout#first": {
    "children": ["disclosure-trigger#first", "disclosure-content#first"]
    
  },
  "disclosure-trigger#first": {
    "children": ["disclosure-state-indicator","rich-text#question1"],
    "props":{
      "blockClass": "pregunta"
    }
    
  },
  "disclosure-content#first": {
    "children": ["rich-text#answer1"],
    "props":{
      "blockClass": "contenido"
    }
  },
  "rich-text#question1": {
    "props": {
      "text": "How can I change my shipping address?"
    }
  },
  "rich-text#answer1": {
    "props": {
      "text": "Call us at (212) 123-1234.",
      "blockClass": "texto-preguntas-frecuentes"
    }
  },



