    "children": ["disclosure-trigger#second", "disclosure-content#second"]
  },
  "disclosure-trigger#second": {
    "children": ["disclosure-state-indicator","rich-text#question2"],
    "props":{
      "blockClass": "pregunta"
    }
  },
  "disclosure-content#second": {
    "children": ["rich-text#answer2"],
    "props":{
      "blockClass": "contenido"
    }
  },
  "rich-text#question2": {
    "props": {
      "text": "can I track my order?"
    }
  },
  "rich-text#answer2": {
    "props": {
      "text": "After logging in to your account, you can find this information in the Orders link.",
      "blockClass": "texto-preguntas-frecuentes"
    }
  },

