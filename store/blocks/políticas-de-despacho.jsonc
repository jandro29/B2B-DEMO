    ]
  },

  "responsive-layout.mobile#politica-de-despacho": {
    "children": [
      "flex-layout.row#titulo-politica-de-despacho",
      "flex-layout.row#section1-politica",
      "flex-layout.row#section2-politica",
      "flex-layout.row#section3-politica",
      "flex-layout.row#section-cuadro1",
